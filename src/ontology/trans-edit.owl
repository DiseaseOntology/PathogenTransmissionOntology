# Class: obo:TRANS_0000000 (transmission process)

AnnotationAssertion(Annotation(oboInOwl:hasDbXref "https://courses.lumenlearning.com/microbiology/chapter/modes-of-disease-transmission/") Annotation(oboInOwl:hasDbXref "https://www.cdc.gov/csels/dsepd/ss1978/lesson1/section10.html"^^xsd:string) obo:IAO_0000115 obo:TRANS_0000000 "A process that is the means during which the pathogen is transmitted directly or indirectly from its natural reservoir, a susceptible host or source to a new host."^^xsd:string)
AnnotationAssertion(oboInOwl:hasOBONamespace obo:TRANS_0000000 "transmission_process"^^xsd:string)
AnnotationAssertion(oboInOwl:id obo:TRANS_0000000 "TRANS:0000000"^^xsd:string)
AnnotationAssertion(rdfs:label obo:TRANS_0000000 "transmission process"^^xsd:string)

# Class: obo:TRANS_0000001 (direct transmission)

AnnotationAssertion(Annotation(oboInOwl:hasDbXref "https://dhss.delaware.gov/dph/files/directindtranspi.pdf") Annotation(oboInOwl:hasDbXref "https://www.cdc.gov/csels/dsepd/ss1978/lesson1/section10.html"^^xsd:string) obo:IAO_0000115 obo:TRANS_0000001 "Direct transmission is a transmission process that is the means during which the pathogen is transmitted immediately from a reservoir, source or host to another host."^^xsd:string)
AnnotationAssertion(oboInOwl:hasOBONamespace obo:TRANS_0000001 "transmission_process"^^xsd:string)
AnnotationAssertion(oboInOwl:id obo:TRANS_0000001 "TRANS:0000001"^^xsd:string)
AnnotationAssertion(rdfs:label obo:TRANS_0000001 "direct transmission"^^xsd:string)
SubClassOf(obo:TRANS_0000001 obo:TRANS_0000000)

