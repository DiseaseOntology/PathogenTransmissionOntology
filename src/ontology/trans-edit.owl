AnnotationAssertion(oboInOwl:hasOBONamespace obo:TRANS_0000005 "transmission_process"^^xsd:string)
AnnotationAssertion(oboInOwl:id obo:TRANS_0000005 "TRANS:0000005"^^xsd:string)
